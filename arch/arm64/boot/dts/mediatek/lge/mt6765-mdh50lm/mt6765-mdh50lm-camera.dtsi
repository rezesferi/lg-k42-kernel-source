	pinctrl-13 = <&camera_pins_cam_ldo_cam2_vcamd_1>;
	pinctrl-14 = <&camera_pins_cam_ldo_cam2_vcamd_0>;
	pinctrl-15 = <&camera_pins_cam_ldo_cam2_vcamd_1>;
