&odm {
	tboard_thermistor3: thermal-sensor3 {
		compatible = "lge,mtboard-thermistor3";
		io-channels = <&auxadc 2>;
		io-channel-names = "thermistor-ch2";
		interconnects = <&auxadc 2>;
	};
};
