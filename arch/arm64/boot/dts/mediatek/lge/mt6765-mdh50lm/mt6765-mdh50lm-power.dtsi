			"charger", "usbid_type", "/sys/devices/platform/odm/odm:usb_id/type",
			/* ccd group */
			"ccd", "icl", "/sys/devices/platform/odm/odm:charger_controller/ccd_icl",
			"ccd", "fcc", "/sys/devices/platform/odm/odm:charger_controller/ccd_fcc",
